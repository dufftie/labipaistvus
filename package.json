{
  "name": "labipaistvus",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx supabase start\" \"pnpm --filter transparency-fe dev\" --names \"supabase,frontend\" --prefix-colors \"blue,green\"",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "bootstrap": "pnpm --filter main bootstrap",
    "crawl": "pnpm --filter crawler crawl",
    "db:migrate": "supabase db push",
    "db:diff": "supabase db diff $npm_config_name --use-migra && pnpm db:generate-types",
    "db:generate-types": "pnpm --filter @labipaistvus/database generate-types",
    "db:reset": "supabase db reset"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "^3.5.3",
    "supabase": "2.76.8",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=18.0.0"
  }
}